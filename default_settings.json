{
	"global.loopInterval": 3,
	"global.enableBoardScroll": true,
	"global.enableTabKeyInTextarea": false,
	"global.enableMarkdownChecklist": false,
	"global.enableMarkdownTables": false,
	"module.listsCollapsing.collapseEmptyLists": false,
	"module.coloredLists.scheme": [
		{
			"pattern": "todo",
			"color": "#eff5d0cc",
			"color_dark": "#2f3617cc"
		}, {
			"pattern": "(sprint|stories)",
			"color": "#d0dff6cc",
			"color_dark": "#09182fcc"
		}, {
			"pattern": "backlog",
			"color": "#c0e8edcc",
			"color_dark": "#123a40cc"
		}, {
			"pattern": "(progress|working|doing)",
			"color": "#bfe3c6cc",
			"color_dark": "#1c4023cc"
		}, {
			"pattern": "(done|ready)",
			"color": "#d9f0bfcc",
			"color_dark": "#313e1ecc"
		}, {
			"pattern": "test",
			"color": "#f5f5c0cc",
			"color_dark": "#3e3e0acc"
		}, {
			"pattern": "fix",
			"color": "#f9c0d0cc",
			"color_dark": "#410606cc"
		}, {
			"pattern": "upgrade",
			"color": "#e6ccf5cc",
			"color_dark": "#240a33cc"
		}, {
			"pattern": "helpdesk",
			"color": "#f5d3f3cc",
			"color_dark": "#2e0a0acc"
		}
	],
	"module.cardsPrioritization.color.critical": "#d91d1d",
	"module.cardsPrioritization.color.high": "#fe9900",
	"module.cardsPrioritization.color.medium": "#f3ee07",
	"module.cardsPrioritization.color.low": "#42ef38",
	"module.cardsPrioritization.color.lowest": "#50bafb",
	"module.scrumTimes.color.bgTeam1Estimation": "#9fd1df",
	"module.scrumTimes.color.fontTeam1Estimation": "#17394d",
	"module.scrumTimes.color.bgTeam1Consumption": "#abdf9f",
	"module.scrumTimes.color.fontTeam1Consumption": "#17394d",
	"module.scrumTimes.color.bgTeam2Estimation": "#dfad9f",
	"module.scrumTimes.color.fontTeam2Estimation": "#17394d",
	"module.scrumTimes.color.bgTeam2Consumption": "#d39fdf",
	"module.scrumTimes.color.fontTeam2Consumption": "#17394d",
	"module.scrumTimes.storyPointsSequence": [ 0, 0.5, 1, 2, 3, 5, 8, 13, "?" ],
	"board.*": {
		"coloredLists": false,
		"showCardsCounter": false,
		"showCardShortId": false,
		"customTags": false,
		"displayMode": "default",
		"scrumTimes": false,
		"scrumTimes.show.team1": true,
		"scrumTimes.show.team2": false,
		"scrumTimes.show.estimation": true,
		"scrumTimes.show.consumption": true,
		"scrumSumTimes": false,
		"cardsSeparator": true
	},
	"list.*": {
		"hidden": null
	}
}